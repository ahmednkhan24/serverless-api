{
  "name": "serverless-api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmednkhan24/serverless-api"
  },
  "scripts": {
    "test": "jest --coverage",
    "pretty": "prettier './src/**/*.{js,ts}' --write",
    "typecheck": "tsc --project tsconfig.prod.json",
    "precompile": "rm -rf dist",
    "compile": "tsc -p tsconfig.prod.json",
    "postcompile": "cp package.json dist && cp yarn.lock dist",
    "build": "yarn compile && sam build",
    "prestart": "yarn build",
    "start": "sam local start-api",
    "predeploy": "yarn build",
    "deploy": "sam deploy --region us-east-2 --stack-name serverless-api-dev --resolve-s3 --capabilities CAPABILITY_IAM",
    "delete-stack": "env PAGER=cat aws cloudformation delete-stack --region us-east-2 --stack-name serverless-api-dev"
  },
  "dependencies": {
    "aws-sdk": "^2.726.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.177",
    "@types/node": "^12.6.8",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.4",
    "typescript": "^3.5.3"
  }
}
